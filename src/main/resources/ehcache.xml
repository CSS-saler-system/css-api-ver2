<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns="http://www.ehcache.org/v3"
        xsi:schemaLocation="http://www.ehcache.org/v3 http://www.ehcache.org/schema/ehcache-core-3.0.xsd">
    <cache alias="requestByCollaborator">
        <key-type>java.util.ArrayList</key-type>
        <value-type>com.springframework.csscapstone.payload.response_dto.PageImplResDto</value-type>
        <expiry>
            <ttl>30</ttl>
        </expiry>
        <resources>
            <heap>10</heap>
            <offheap unit="MB">10</offheap>
        </resources>
    </cache>
    <cache alias="requestByStatusAndEnterprise">
        <key-type>java.util.ArrayList</key-type>
        <value-type>com.springframework.csscapstone.payload.response_dto.PageImplResDto</value-type>
        <expiry>
            <ttl>30</ttl>
        </expiry>
        <resources>
            <heap>10</heap>
            <offheap unit="MB">10</offheap>
        </resources>
    </cache>
    <cache alias="requestById">
        <key-type>java.util.ArrayList</key-type>
        <value-type>com.springframework.csscapstone.payload.response_dto.PageImplResDto</value-type>
        <expiry>
            <ttl>30</ttl>
        </expiry>
        <resources>
            <heap>10</heap>
            <offheap unit="MB">10</offheap>
        </resources>
    </cache>

    <cache alias="getAllRequest">
        <key-type>java.lang.String</key-type>
        <value-type>java.util.List</value-type>
        <expiry>
            <ttl>30</ttl>
        </expiry>
        <resources>
            <heap>10</heap>
            <offheap unit="MB">10</offheap>
        </resources>
    </cache>

</config>